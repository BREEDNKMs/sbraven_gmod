"VertexLitGeneric"
{
	"$basetexture" "models\alvaroports\SBRaven\Torso-3"
	"$bumpmap" "models\alvaroports\SBRaven\Torso-3_normal"
	"$phongexponenttexture" "models\alvaroports\SBRaven\Torso-3_roughness"

	"$phong" "1"
	"$phongexponentfactor" "30"
//	"$phongexponent" "10" 					 // (controla o espalhamento, maior=menos espalhamento, metal polido ou algo assim)
	"$phongboost" "1" 					 // controle de reflexo/quanto mais alto = mais molhado/reflexo
	"$phongfresnelranges" "[.1 .5 1]" 	 // força do phong, maior os numeros = phong mais branco e intenso
//	"$phongfresnelranges" "[0.1 0.5 1]" 	 // força do phong, maior os numeros = phong mais branco e intenso
//	"$phongalbedotint" "1" 					 // aplicar phong a textura base, desativar se quiser o phong mais branco

	"$rimlight" "1"
	"$rimlightexponent" "1"     // non functional 
	"$rimlightboost" "1"        // like the phongexponent, it controls the spreading 
	"$rimmask" "1"				// Uses the alpha channel of the phongexponenttexture as specular (useless if using dxt1 without alpha) 

	"$nodecal"					"1"
//	"$alphatest"				"1"
	"$allowAlphaToCoverage"		"1"
//	"$blendtintbybasealpha" "1" // apply color2 only to the alpha of the base texture 
	"$color2" "[1 0.938 0.913]"
}