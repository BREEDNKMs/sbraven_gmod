$sequence "walk_cautious" {
	"a_walking_walk_cautious_S"  // row 1 col 1 (SW slot → backward)
	"a_walking_walk_cautious_S"  // row 1 col 2 (S slot)
	"a_walking_walk_cautious_S"  // row 1 col 3 (SE slot)

	"a_walking_walk_cautious_W"  // row 2 col 1 (W slot)
	"a_walking_walk_cautious_C"  // row 2 col 2 (Center still)
	"a_walking_walk_cautious_E"  // row 2 col 3 (E slot)

	"a_walking_walk_cautious_N"  // row 3 col 1 (NW slot)
	"a_walking_walk_cautious_N"  // row 3 col 2 (N slot → forward)
	"a_walking_walk_cautious_N"  // row 3 col 3 (NE slot)
	
	activity "ACT_MP_WALK_MELEE" 1
	blend "move_y" 1 -1
	blend "move_x" -1 1
	blendwidth 3
	fadein 0.3
	fadeout 0.3
	addlayer inverse_proportions 
	node "walking" 
	fudgeloop 
	{ event AE_NPC_LEFTFOOT 10 "" }
	{ event AE_NPC_RIGHTFOOT 28 "" }
} 

